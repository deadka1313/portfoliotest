.contact_icon
  display inline-block
  width 17.36%
  svg 
    width 100%
    height 100%

.contact_title
  display inline-block
  position relative
  font-family MyriadPro-bold  
  letter-spacing 0.2px
  color #3d6cb0
  font-size: 45px
  vertical-align top
  padding-top 12px
  padding-left 10px
  padding-right 8px
  background white
  z-index 50

.contact_line
  position absolute
  width 50%
  right 0
  top 35px
  height 5px
  background #3d6cb0

.contact_all
  position relative

.contact_address__left
.contact_e-mail__left
.contact_phone__left
.contact_website__left
  display inline-block
  font-family MyriadPro-bold 
  width 31.42%
  font-size 22.66px 
  color #3d6cb0

.contact_address__right
.contact_e-mail__right
.contact_phone__right
.contact_website__right
  display inline-block
  font-family MyriadPro-regular  
  font-size 22.66px 
