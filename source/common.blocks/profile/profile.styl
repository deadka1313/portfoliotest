.profile
  &_icon
    display inline-block
    width 18.02%
    svg 
      width 100%
      height 100%

  &_title
    display inline-block
    position relative
    font-family MyriadPro-bold  
    letter-spacing 0.2px
    color mainColor
    font-size: 45px
    vertical-align top
    padding-top 12px
    padding-left 7px
    padding-right 8px
    background white
    z-index 50

  &_line
    position absolute
    width 50%
    right 0
    top 36px
    height 5px
    background mainColor

  &_all
    position relative

  &_text
    padding-top 10px
    line-height 20.5px
    letter-spacing 0.01px
    margin-bottom 51px

  &_text span
    font-family MyriadPro-bold  
    line-height 20.8px
    letter-spacing 0.01px